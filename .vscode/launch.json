{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "launch_chrome",
            "url": "http://localhost:5173/example/index.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "attach_chrome",
            "port": 5173,
            "url": "http://localhost:5173/example/index.html",
            "webRoot": "${workspaceFolder}"
        }
    ]
}